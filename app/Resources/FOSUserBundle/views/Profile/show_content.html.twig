{% trans_default_domain 'FOSUserBundle' %}


<div class="fos_user_user_show">
</div>


{% block body %}
    <html class="no-js" xmlns="http://www.w3.org/1999/html"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>Onécho</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">
        <link rel='shortcut icon' type='image/x-icon' href='favicon.ico' />
        <link rel="apple-touch-icon-precomposed" href="apple-touch-icon-precomposed.png">
        <link rel="apple-touch-icon-precomposed" href="apple-touch-icon-72x72-precomposed.png">
        <link rel="apple-touch-icon-precomposed" href="apple-touch-icon-114x114-precomposed.png">
        <link rel="apple-touch-icon-precomposed" href="apple-touch-icon-144x144-precomposed.png">
        <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">

        <link rel="stylesheet" href="/octicons/octicons.css">
        <link rel="stylesheet" href="{{ asset('bundles/hackathon/css/normalize.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/hackathon/css/main.css') }}">
        <!-- Appel des bibliothèques jQuery -->
        <script type="text/javascript" src="http://code.jquery.com/jquery-latest.min.js"
                xmlns="http://www.w3.org/1999/html"></script>
        <script type="text/javascript"
                src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.14/jquery-ui.min.js"></script>
        <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.10.1.min.js"><\/script>')</script>
        <script src="{{ asset('bundles/hackathon/js/vendor/jquery.hashchange.min.js') }}"></script>
        <script src="{{ asset('bundles/hackathon/js/vendor/jquery.easytabs.min.js') }}"></script>

        <script src="{{ asset('bundles/hackathon/js/main.js') }}"></script>
        <!--[if lt IE 9]>
        <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <script>window.html5 || document.write('<script src="js/vendor/html5shiv.js"><\/script>')</script>
        <![endif]-->
    </head>
    <body class="bg-fixed bg-1">
    <div class="main-container">
        <div class="main wrapper clearfix">
            <!-- Header Start -->
            <header id="header">
                <div id="logo">
                    <h2>
                        {{ user.username }}
                    </h2>


                </div>
            </header>
            <!-- Header End -->
            <!-- Main Tab Container -->
            <div id="tab-container" class="tab-container">
                <!-- Tab List -->
                <ul class='etabs'>
                    <li class='tab' id="tab-about">
                        <a href="#about"><i class="icon-user"></i><span>Profil</span></a>
                    </li>
                    <li class='tab'>
                        <a href="#resume"><i class="icon-file-text"></i><span>Amis</span></a>
                    </li>
                    <li class='tab'>
                        <a href="#portfolio"><i class="icon-heart"></i><span>Social Hub</span></a>
                    </li>

                </ul>
                <!-- End Tab List -->
                <div id="tab-data-wrap">
                    <!-- About Tab Data -->
                    <div id="about">
                        <section class="clearfix">
                            <div class="g2">
                                <div class="photo">
                                    <img src={{ user.photo }} alt="photo de profil">
                                </div>
                                <div class="info">
                                    <h3>
                                        {{ user.username }}
                                    </h3>



                                </div>

                                    <p><span>Prénom :</span>{{ user.prenom }}</p>
                                    <p><span>Nom :</span>{{ user.nom }}</p>
                                    <p><span>Age :</span>{{ user.age }}</p>
                                    <p><span>Ville :</span>{{ user.ville }}</p>


                                <div class="tuile">
                                    <div class="title"><a href="{{ path('hackathon_mesvacances') }}">Mes Prochaines Vacances</a></div>
                                </div>


                            </div>
                            <div class="g1">

                            </div>
                            <div class="break"></div>
                            <div class="contact-info">
                                <div class="g1">
                                    <div class="item-box clearfix">
                                        <i class="icon-envelope"></i>
                                        <div class="item-data">
                                            <h3><a href="mailto:{{ user.email }}">{{ user.email }}</a></h3>
                                            <p>Email</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="g1">
                                    <div class="item-box clearfix">
                                        <i class="icon-facebook"></i>
                                        <div class="item-data">
                                            <h3><a {{ user.facebook }} target="_blank">Facebook</a></h3>
                                            <p>Facebook</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="g1">
                                    <div class="item-box clearfix">
                                        <i class="icon-twitter"></i>
                                        <div class="item-data">
                                            <h3><a href="http://twitter.com/{{ user.twitter }}" target="_blank">{{ user.twitter }}</a></h3>
                                            <p>Twitter</p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section><!-- content -->
                    </div>
                    <!-- End About Tab Data -->
                    <!-- Resume Tab Data -->
                    <div id="resume">
                        <section class="clearfix">
                            <div class="g2">
                                <ul class="no-list work">
                                    <li>
                                        <h5>
                                            Mes Amis
                                        </h5>

                                        <p>Work in Progress</p>

                                        <div class="meter emerald">
                                            <span style="width: 25%"></span>
                                        </div>
                                        
                                    </li>

                                </ul>
                            </div>
                            </section>

                    </div>

                    <!-- End Resume Tab Data -->
                    <!-- Portfolio Tab Data -->
                            <div id="portfolio">
                                <section class="clearfix">

                                    <div class="break"></div>
                                    <div class="g1">
                                        <div class="image">
                                            <img src="{{ asset('bundles/hackathon/images/photos1.png') }}" alt="image">
                                            <div class="image-overlay">
                                                <div class="image-link">
                                                    <a href="../../bundles/hackathon/images/photos1.png" class="btn"><i class="fa fa-search" aria-hidden="true">ZOOM</i></a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="g1">
                                        <div class="image">
                                            <img src="{{ asset('bundles/hackathon/images/photos2.png') }}" alt="image">
                                            <div class="image-overlay">
                                                <div class="image-link">
                                                    <a href="../../bundles/hackathon/images/photos2.png" class="btn"><i class="fa fa-search" aria-hidden="true"></i>ZOOM</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="g1">
                                        <div class="image">
                                            <img src="{{ asset('bundles/hackathon/images/photos3.png') }}" alt="image">
                                            <div class="image-overlay">
                                                <div class="image-link">
                                                    <a href="../../bundles/hackathon/images/photos3.png" class="btn"><i class="fa fa-search" aria-hidden="true"></i>ZOOM</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </section>
                            </div>
                    <!-- End Portfolio Data -->


            <!-- End Tab Container -->
            <footer>

            </footer>
        </div><!-- #main -->
    </div><!-- #main-container -->



    </body>
    </html>

{% endblock %}
